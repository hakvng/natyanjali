0% { -webkit-transform: translateX(0px); }

20% { -webkit-transform: translateX(0px); }

25% { -webkit-transform: translateX(-682px); }

45% { -webkit-transform: translateX(-682px); }

50% { -webkit-transform: translateX(-1364px); }

70% { -webkit-transform: translateX(-1364px); }

75% { -webkit-transform: translateX(-2048px); }

95% { -webkit-transform: translateX(-2048px); }

100% { -webkit-transform: translateX(-2732px);  }


}
	
@-moz-keyframes horizontalslider {

0% { -moz-transform: translateX(0px); }

20% { -moz-transform: translateX(0px); }

25% { -moz-transform: translateX(-682px); }

45% { -moz-transform: translateX(-682px); }

50% { -moz-transform: translateX(-1364px); }

70% { -moz-transform: translateX(-1364px); }

75% { -moz-transform: translateX(-2048px); }

95% { -moz-transform: translateX(-2048px); }

100% { -moz-transform: translateX(-2732px);  }


}







